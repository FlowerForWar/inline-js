{
  "name": "inline-js",
  "version": "0.5.0",
  "description": "Static assets inliner",
  "author": "eight04 <eight04@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.js --ignore-pattern !.inline.js --cache",
    "mocha": "mocha --require intelli-espower-loader",
    "mocha-debug": "mocha --require intelli-espower-loader --reporter spec --inspect-brk",
    "test": "npm run lint && npm run mocha",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push --follow-tags && npm publish",
    "build": "node cli.js README.md -o README.md",
    "prepack": "crlf --set=LF cli.js"
  },
  "bin": {
    "inlinejs": "cli.js"
  },
  "main": "index.js",
  "dependencies": {
    "asyncro": "^2.0.1",
    "clean-css": "^4.1.9",
    "fs-extra": "^5.0.0",
    "is-binary-path": "^2.0.0",
    "js-tokens": "^3.0.1",
    "mime": "^2.1.0",
    "neodoc": "^1.4.0",
    "treeify": "^1.0.1"
  },
  "devDependencies": {
    "crlf": "^1.1.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^4.0.1",
    "power-assert": "^1.4.4",
    "proxyquire": "^1.8.0",
    "sinon": "^4.1.3"
  },
  "repository": "eight04/inline-js"
}
